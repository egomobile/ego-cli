{
    "description": "A free source-code editor made for Windows, Linux and macOS.",
    "homepage": "https://code.visualstudio.com/",
    "maintainer": {
        "name": "Marcel Kloubert",
        "homepage": "https://github.com/mkloubert",
        "contacts": ["mailto:marcel.kloubert@gmx.net"]
    },
    "author": {
        "name": "Microsoft",
        "homepage": "https://www.microsoft.com/"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/microsoft/vscode"
        }
    ],
    "sources": {
        "debian,ubuntu": {
            "386,amd64": {
                "requirements": [
                    "apt://wget",
                    "apt://apt-transport-https"
                ],
                "source": "apt://code"
            }
        },
        "darwin": {
            "*": {
                "source": "brew://visual-studio-code"
            }
        },
        "windows": {
            "amd64": {
                "source": "winget://Microsoft.VisualStudioCode"
            }
        }
    }
}